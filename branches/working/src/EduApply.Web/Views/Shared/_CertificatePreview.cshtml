@model IEnumerable<EduApply.Data.Entities.Certificate>
<div class="container">
    <hr/>
    <h4>Certificate</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger alert alert-danger" })
    @foreach (var cert in Model)
    {
        <div class="form-group">
            <div class="col-md-8"><strong>Certificate Name: </strong>@cert.CertificateName</div>
            <div class="col-md-4"><strong>Certificate Type: </strong>@cert.CertificateType</div>
            <div class="row"></div>
        </div>
    }

</div>